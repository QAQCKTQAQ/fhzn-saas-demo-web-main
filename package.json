{
	"name": "react",
	"private": true,
	"version": "0.0.1",
	"scripts": {
		"start": "run-p serve dev",
		"dev": "vite",
		"build:dev": "tsc && vite build --mode development",
		"build:test": "tsc && vite build --mode test",
		"build:pro": "set NODE_OPTIONS= --max_old_space_size=8192 && tsc && vite build --mode production",
		"serve": "nodemon server",
		"preview": "vite preview",
		"lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
		"lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\" \"server/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
		"lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
		"lint:lint-staged": "lint-staged",
		"prepare": "husky install",
		"tsc": "tsc",
		"release": "standard-version",
		"commit": "git pull && git add -A && git-cz && git push"
	},
	"dependencies": {
		"@koa/cors": "^4.0.0",
		"@react-three/drei": "^9.74.16",
		"@react-three/fiber": "^8.13.3",
		"antd": "^4.24.7",
		"axios": "^0.27.2",
		"cesium": "^1.110.0",
		"classnames": "^2.3.2",
		"cropperjs": "^1.5.13",
		"driver.js": "^0.9.8",
		"echarts": "^5.3.0",
		"echarts-gl": "^2.0.9",
		"echarts-liquidfill": "^3.1.0",
		"form-data": "^4.0.0",
		"i18next": "^23.2.11",
		"immer": "^9.0.15",
		"js-md5": "^0.7.3",
		"js-yaml": "^4.1.0",
		"jsencrypt": "^3.3.2",
		"koa": "^2.14.1",
		"koa-body": "^6.0.1",
		"koa-bodyparser": "^4.4.0",
		"koa-compress": "^5.1.0",
		"koa-favicon": "^2.1.0",
		"koa-router": "^7.0.1",
		"koa-session": "^6.4.0",
		"koa-static": "^5.0.0",
		"lamina": "^1.1.23",
		"leva": "^0.9.35",
		"lodash": "^4.17.21",
		"nanoid": "^4.0.2",
		"nodemon": "^2.0.22",
		"nprogress": "^0.2.0",
		"pubsub-js": "^1.9.4",
		"qs": "^6.10.5",
		"react": "^18.2.0",
		"react-activation": "^0.11.2",
		"react-dom": "^18.2.0",
		"react-i18next": "^13.0.2",
		"react-json-view": "^1.21.3",
		"react-redux": "^8.0.2",
		"react-router-dom": "^6.3.0",
		"react-transition-group": "^4.4.2",
		"redux": "^4.2.0",
		"redux-persist": "^6.0.0",
		"redux-promise": "^0.6.0",
		"redux-thunk": "^2.4.1",
		"resium": "^1.17.2",
		"screenfull": "^6.0.2",
		"simplex-noise": "^4.0.1",
		"vite-plugin-cesium": "^1.2.22"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.2",
		"@commitlint/config-conventional": "^17.0.2",
		"@types/lodash": "^4.14.192",
		"@types/pubsub-js": "^1.8.3",
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"@types/react-router-dom": "^5.3.3",
		"@types/redux-promise": "^0.5.29",
		"@types/three": "^0.152.1",
		"@typescript-eslint/eslint-plugin": "^5.27.1",
		"@typescript-eslint/parser": "^5.27.1",
		"@vitejs/plugin-react": "^1.3.0",
		"autoprefixer": "^10.4.7",
		"commitizen": "^4.2.4",
		"cz-git": "^1.3.4",
		"eslint": "^8.17.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.30.0",
		"eslint-plugin-react-hooks": "^4.5.0",
		"husky": "^8.0.1",
		"koa-connect": "^2.1.0",
		"less": "^4.1.3",
		"lint-staged": "^13.0.2",
		"moment": "^2.29.4",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"rollup-plugin-visualizer": "^5.6.0",
		"standard-version": "^9.5.0",
		"stylelint": "^14.9.1",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-recess-order": "^3.0.0",
		"stylelint-config-standard": "^26.0.0",
		"stylelint-less": "^1.0.6",
		"three": "^0.153.0",
		"typescript": "^4.6.3",
		"vite": "^4.5.0",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-eslint": "^1.6.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-svg-icons": "^2.0.1"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	},
	"overrides": {
		"react-json-view": {
			"react": "$react",
			"react-dom": "$react-dom"
		}
	}
}
